
<% layout('layout')%>
<div class="container">
    <h1>Ask a question</h1>
    <hr>
    <div class="row">
        <div class="col-lg-6">
            
            <form method="post">
                <div class="form-group">
                    <label for="name">Subject : </label>
                    <input class="form-control" name="textone" type="text">
                </div>
                <div class="form-group">
                    <label for="text">Body : </label>
                    <input class="form-control" name="texttwo" type="text">
                </div>
                
                <div class="form-group">
                    <button type="submit" class="btn btn-success">Submit</button>
                </div>
            </form>
        </div>
    </div>
</div>



 
